<?xml version="1.0" encoding="utf-8" ?>
<diff>
	<!--
		add:
		<execute_trade tradeoffer="$tradedeal" tradepartner="this.ship" amount="$amount" result="$traderesult" custom="true" comment="Deal with fixed price" />
	-->
	<add pos="after" sel="//execute_trade">
		<do_if value="$traderesult">
			<signal_objects object="player.galaxy" param="'kTAnalytics_trade'" param2="table[
				$tradeOffer=$tradedeal,
				$tradePartner=this.ship,
				$amount=$amount,
				$result=$traderesult,
				$aiScriptId=this.object.order.id
			]" />
		</do_if>
	</add>
</diff>
